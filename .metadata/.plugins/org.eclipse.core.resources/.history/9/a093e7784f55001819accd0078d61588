		System.out.println("Start time :'"+System.currentTimeMillis());
		for(int i=0; i<100;i++) {
			System.out.print(" " +i);
		}
		System.out.println("End time :'"+System.currentTimeMillis());
		
		System.out.println("Start time :'"+System.currentTimeMillis());
		int[] arr = new int[100];
		for(int i : arr) {
			System.out.print(" " +i);
		}
		System.out.println("End time :'"+System.currentTimeMillis());
		
	}

}
